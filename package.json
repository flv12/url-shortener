{
  "name": "url-shortener",
  "version": "0.0.1",
  "description": "A simple Node app with SQLite as a database, shorten your url fast and easy",
  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "build-dev": "parcel build src/index.html --out-dir dist",
    "build-prod": "parcel build src/index.html --out-dir dist --no-source-maps",
    "dev": "npm run build-dev && nodemon server.js",
    "prod": "npm run build-prod && node server.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nanoid": "^3.1.15",
    "parcel": "^1.12.4",
    "sqlite3": "^5.0.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "engines": {
    "node": "12.x"
  },
  "license": "MIT"
}
